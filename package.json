{
    "name": "webflow-ui",
    "version": "0.0.3",
    "description": "A modern, accessible Vue 3 component library built with TypeScript, Tailwind CSS, and Radix Vue",
    "author": "Reda21",
    "license": "MIT",
    "keywords": [
        "vue",
        "vue3",
        "components",
        "ui",
        "component-library",
        "typescript",
        "tailwindcss",
        "radix-vue",
        "accessible",
        "form-validation",
        "button",
        "card",
        "input",
        "drawer",
        "tooltip",
        "avatar",
        "badge",
        "datepicker"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/reda21/index.git"
    },
    "bugs": {
        "url": "https://github.com/reda21/index/issues"
    },
    "homepage": "https://github.com/reda21/index#readme",
    "type": "module",
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "development": "./src/index.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs"
        },
        "./src": "./src/index.ts",
        "./src/*": "./src/*",
        "./style.css": "./dist/index.css",
        "./dist/index.css": "./dist/index.css",
        "./nuxt": "./src/nuxt.ts",
        "./dist/*": "./dist/*"
    },
    "sideEffects": [
        "*.css"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "typecheck": "vue-tsc --noEmit",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.7.4",
        "@floating-ui/vue": "^1.1.9",
        "@iconify/vue": "^5.0.0",
        "@primeuix/themes": "^2.0.2",
        "@types/three": "^0.182.0",
        "chart.js": "^4.5.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "primevue": "^4.5.1",
        "radix-vue": "^1.9.17",
        "tailwind-merge": "^3.4.0",
        "tailwindcss-primeui": "^0.6.1",
        "three": "^0.182.0",
        "vaul-vue": "^0.4.1",
        "vue": "^3.5.25"
    },
    "optionalDependencies": {
        "yup": "^1.7.1",
        "zod": "^3.0.0 || ^4.0.0"
    },
    "devDependencies": {
        "@nuxt/kit": "^4.2.2",
        "@nuxt/schema": "^4.2.2",
        "@tailwindcss/vite": "^4.1.17",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vitest/ui": "^4.0.14",
        "@vue/test-utils": "^2.4.6",
        "happy-dom": "^20.0.11",
        "tailwindcss": "^4.1.17",
        "tsup": "^8.5.1",
        "typescript": "^5.7.2",
        "vite": "^6.4.1",
        "vite-plugin-dts": "^4.3.0",
        "vitest": "^4.0.14",
        "vue-tsc": "^2.2.0",
        "yup": "^1.7.1",
        "zod": "^4.1.13"
    }
}